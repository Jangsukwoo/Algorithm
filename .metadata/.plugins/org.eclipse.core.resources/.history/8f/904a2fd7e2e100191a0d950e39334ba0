package 구름;

import java.util.Scanner;

public class 계수기만들기 {
	static int m;
	static int[] num;
	static int[] max;
	static int count;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		m = sc.nextInt();
		num = new int[m];
		max = new int[m];
		for(int i=0;i<m;i++) max[i] = sc.nextInt();
		for(int i=0;i<m;i++) num[i] = sc.nextInt();
		count = sc.nextInt();
	}
}
