package DP;
/*
 * knapSack
 */
import java.util.Scanner; 
public class 수강과목{ 
	static Scanner sc=new Scanner(System.in); 
	public static void main(String[] args){ 
		int maxtime=sc.nextInt(); 
		int K=sc.nextInt(); 
		int[][] arr=new int[K+1][K+1]; 
		Subject[] subject=new Subject[K+1]; 
		for(int i=1;i<=K;i++) 
			subject[i]=new Subject(sc.nextInt(),sc.nextInt()); 
		for(int i=1;i<=K;i++){
			for(int j=1;j<=K;j++){ 
				if(subject[i].getimportant()<=j && arr[i-1][j]<=arr[i-1][j-subject[i].getimportant()]+subject[i].gettime()) 
					arr[i][j]=arr[i-1][j-subject[i].getimportant()]+subject[i].gettime(); 
				else 
					arr[i][j]=arr[i-1][j]; 
			} 
		} 
		for(int i=1;i<=K;i++){ 
			for(int j=1;j<=important;j++) 
				System.out.print(arr[i][j]+" "); 
			System.out.println(); 
		} 
	} 
} 
class Subject{
	private int important; 
	private int time; 
	Subject(int important, int time){ 
		this.important=important; 
		this.time=time; 
	} 
	public int getimportant(){ 
		return important; 
	} 
	public int gettime(){ 
		return time; 
	} 
}