package Samsung;

import java.util.Scanner;
/*
 * 3:55~
 * 
 */
public class 게리맨더링2 {
	static int N;
	static int[][] A;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		A = new int[N][N];
		for(int row=0;row<N;row++)
			for(int col=0;col<N;col++) A[row][col] = sc.nextInt();
		
	}
}
