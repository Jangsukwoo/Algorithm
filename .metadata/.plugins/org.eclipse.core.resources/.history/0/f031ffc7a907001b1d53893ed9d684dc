package SamsungDS;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

/*
 * 16:40~
 * 총 3개의 점화식
 * 
 * an = 공차 1 등차수열
 * 
 * an = 등비 2인 등비수열
 * 
 * an = a(n-1) * 3 + 1
 */

public class IQTest {
	static int N;
	static int[] series;
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static int nextNumber;
	public static void main(String[] args) throws NumberFormatException, IOException {
		N = Integer.parseInt(br.readLine());
		series = new int[N];
		st = new StringTokenizer(br.readLine());
		for(int i=0;i<N;i++) series[i] = Integer.parseInt(st.nextToken()); 
		if(N==1) System.out.println("1");
		else if(isRecurrenceFormula()) System.out.println(nextNumber);
		else System.out.println(nextNumber);
	}
	private static boolean isRecurrenceFormula(){
		if(isFirstFormula()) return true;
		if(isSecondFormula()) return true;
		if(isThirdFormula()) return true;
		return false;
	}
	private static boolean isFirstFormula(){
		int difference  = 1; //공차 d = 1
		return false;
	}
	private static boolean isSecondFormula(){
		return false;
	}
	private static boolean isThirdFormula(){
		return false;
	}

}
