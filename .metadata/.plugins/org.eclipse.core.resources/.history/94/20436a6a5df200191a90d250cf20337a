package Samsung;

import java.util.Scanner;

/*
 * 645
 * 침착하고 집중하자
 * 상황파악 ->시뮬?완탐?
 * ->시뮬 -> 구현
 * ->완탐 -> 내부처리 자료구조 고민 + 구현방법 고민
 * 
 * ->구현
 */
class Ladder{
	int row;
	int col;
	public Ladder(int r, int c) {
		row = r;
		col = c;
	}
}
public class 사다리조작 {
	static int N,M,H;//N:세로선 개수 , M은 가로선 개수 , H 가로선 놓을 수 있는 위치 개수
	static int[][] ladder;
	static boolean[][] putLadder;
	static int addLadder;
	static int r;
	static Ladder[] ladderCase;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt()+1;
		M = sc.nextInt();
		H = sc.nextInt()+1;
		ladder = new int[H][N];
		putLadder = new boolean[H][N];
		for(int i=0;i<M;i++){
			int a = sc.nextInt();
			int b = sc.nextInt();
			ladder[a][b]=1;
			ladder[a][b+1]=1;
			putLadder[a][b]=true;
			putLadder[a][b+1]=true;
		}
		for(int ad=1;ad<=3;ad++) {
			r = ad;
			ladderCase = new Ladder[r];
			dfs(0,0);
		}
		
	}
	private static void dfs(int idxRow, int cnt){
		if(cnt==r){
			
			return ;
		}
		
		for(int row=idxRow;row<H;row++){
			for(int col=0;col<N;col++){
				if(putLadder[row][col]==false){ //사다리 안놓여져있고
					if(col>1 && col<N-1){//안쪽인데
						if(ladder[row][col-1]!=1 && ladder[row][col+1]!=1) {
							//이건 놓을 수 있음
							putLadder[row][col]=true;
							putLadder[row][col+1]=true;
							ladder[row][col]=1;
							ladder[row][col+1]=1;
							dfs(idxRow,cnt+1);
							putLadder[row][col]=false;
							putLadder[row][col+1]=false;
							ladder[row][col]=0;
							ladder[row][col+1]=0;
						}
					}
				}
			}
		}
	}
	private static void view() {
		for(int row=1;row<H;row++) {
			for(int col=1;col<N;col++) {
				System.out.print(ladder[row][col]+" ");
			}
			System.out.println();
		}
		System.out.println();
	}
}
