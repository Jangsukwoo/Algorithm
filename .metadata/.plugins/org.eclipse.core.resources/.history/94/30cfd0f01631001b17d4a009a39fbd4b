package CodingTest;

public class 느흐느고도1 {
	static int minCost;
	public static void main(String[] args) {
		
	}
    public static int solution(int[] goods){
    	minCost = Integer.MAX_VALUE;
    	int sum = 0;
    	for(int choose = 1; choose<=3; choose++) {
    		sum = goods[choose];
    	}
    	return 0;
    }
}
