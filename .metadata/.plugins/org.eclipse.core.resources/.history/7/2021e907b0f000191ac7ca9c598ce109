package Samsung;

import java.util.Scanner;
class Pipe{
	int headRow;
	int headCol;
	int tailRow;
	int tailCol;

	public Pipe(int hr,int hc, int tr, int tc) {
		headRow = hr;
		headCol = hc;
		tailRow = tr;
		tailCol = tc;
	}
}
public class 상시_파이프옮기기 {
