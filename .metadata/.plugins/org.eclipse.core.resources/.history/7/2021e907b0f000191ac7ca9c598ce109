	static int[][] house;
	static int[] dr = {0,1,1};
	static int[] dc = {1,1,0}; // 우,우하,하
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		house = new int[N][N];
		
	}
}
