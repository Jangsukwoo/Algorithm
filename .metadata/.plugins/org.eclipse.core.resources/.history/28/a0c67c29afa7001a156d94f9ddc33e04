package 공채대비;

import java.util.HashMap;

public class Monster {
	/*
	 * 사냥꾼은 0,0에 위치
	 * 
	 */
	static HashMap<Double,Integer> target;
	public static void main(String[] args) {
		solution(new int[][] {{2,3},{4,5},{3,-3},{2,-4},{3,-6},{-3,-3},{-5,0},{-4,4}}, new int[][] {{4,1},{4,6},{1,-2},{-4,-4},{-3,0},{-4,4}});
	}
    public static int solution(int[][] monsters, int[][] bullets) {
        int answer = 0;
        target= new HashMap<Double, Integer>();
        for(int i=0;i<monsters.length;i++){//몬스터 기울기 좌표 전부 저장
        	int x = monsters[i][0];
        	int y = monsters[i][1];
        	double gradient = y/x;
        	if(target.containsKey(gradient)){//이미 존재하는 각도에 몬스터가 있으면
        		int population = target.get(gradient);
        		target.put(gradient,population+1);
        	}else target.put(gradient,1);
        }
        
        for(int i=0;i<bullets.length;i++){//사격
        	int x = bullets[i][0];
        	int y = bullets[i][1];
        	double gradient = y/x; //총알의 기울기(각도)
        	if(target.containsKey(gradient)){//각도에 뭔가 존재 
        		int population = target.get(gradient);
        		if(population>0) {
        			
        		}
        		target.put(gradient,population+1);
        	}else target.put(gradient,1);
        }
        return answer;
    }
}
