package Samsung;

import java.util.Scanner;

/* 시작
 * 모든 1을 덮는데 필요한 종이의 최소 개수
 * 
 */
public class 상시_색종이붙이기 {
	static int[][] paper;
	static boolean[][] painted;
	static int startR,startC;
	static int min = Integer.MAX_VALUE;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		paper = new int[10][10];
		for(int row=0;row<10;row++)
			for(int col=0;col<10;col++) paper[row][col] = sc.nextInt();
		dfs();
		
	}
	private static void dfs() {
		if(paperCheck()){
			
			return ;
		}
	}
	private static boolean paperCheck() {
		for(int row=0;row<10;row++) {
			for(int col=0;col<10;col++) {
				if(paper[row][col]==1 && painted[row][col]==true) continue;
				else if(paper[row][col]==1 && painted[row][col]==false) return false;
			}
		}
		return true;
	}
}
