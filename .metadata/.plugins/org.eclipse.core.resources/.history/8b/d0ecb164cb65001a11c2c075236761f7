package CodingStudyHW;

import java.util.Scanner;
/*
 * 물통 A,B,C 세개가 있는데
 * 처음 두 물통 A,B는 비워져있고 세번째 물통 C는 가득 차있다.
 */
public class 물통 {
	static int A,B,C;
	public static void main(String[] args) {
		setData();
	}
	private static void setData() {
		Scanner sc = new Scanner(System.in);
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
	}
}
