package Samsung;

import java.util.Scanner;

public class 기출_경사로 {
	static int N,L;
	static int[][] roadMap;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		L = sc.nextInt();
		roadMap = new int[N][N];
		for(int row=0;row<N;row++)
			for(int col=0;col<N;col++) roadMap[row][col] = sc.nextInt();
	}
}
