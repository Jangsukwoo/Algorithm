package Simulation;
import java.util.Scanner;

public class 문자열화폐진수코드 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int x = sc.nextInt();
		int[] arr = new int[n];
		for (int i = 0; i < n; i++) {
			arr[i] = 1;
		}
		StringBuilder sb = new StringBuilder();
		
		int cnt =n;
		fin:
		for (int i = n-1; i >=0 ; i--) {
			for (int j = 1; j <= 26; j++) {
				cnt++;
				arr[i] = j;
				if(cnt == (x+i)) break fin;
			}
		}
		if(x < n) {
			sb.append('!');
		}else 	if(x < cnt) {
		for (int i = 0; i < n; i++) {
			char a = (char) ('A'+(arr[i]-1));
			sb.append(a);
		}
		}else sb.append('!');
		
		System.out.print(sb.toString());
	}
}