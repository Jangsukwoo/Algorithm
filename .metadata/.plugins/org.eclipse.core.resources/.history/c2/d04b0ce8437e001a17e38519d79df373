package CodingStudy;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.HashSet;
import java.util.LinkedList;
import java.util.Queue;
import java.util.Set;
import java.util.StringTokenizer;

/*
 * 08:05 시작
 * 정수 X를 받고
 * N-1번의 연산을함
 * 적용하는 연산은 두가지임
 * 1. 3으로 나누되 3으로 나누어 떨어져야함
 * 2. 2를 곱함
 * 
 * 만든 수를 기록해나가면 수열 A를 만들 수 있다.
 * 수열 A의 순서를 섞은 수열 B를 입력으로 받았을 떄 
 * 수열 A를 구해보자.
 * 
 */
public class 나3곱2 {
	static long[] seriesB;
	static int N;
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static boolean find;
	static class Series{
		long num;
		String series;
		Set<Long> set;
		public Series(long num, String series, Set<Long> set) {
			this.num = num;
			this.series = series;
			this.set = set;
		}
	}
	public static void main(String[] args) throws NumberFormatException, IOException {
		setData();
		for(int i=0;i<N;i++) {
			bfs(seriesB[i]);
		}
	}
	private static void bfs(long startNumber){
		Series root = new Series(startNumber,Long.toString(startNumber),new HashSet<Long>());
		for(int i=0;i<N;i++) if(seriesB[i]!=startNumber) root.set.add(seriesB[i]);
		Queue<Series> q = new LinkedList<Series>();
		while(!q.isEmpty()){
			int size = q.size();
			for(int i=0;i<size;i++) {
				Series currentSeries = q.poll();
				long currentNumber = currentSeries.num;
				Set<Long> currentSet = currentSeries.set;
				if(currentSet.contains(currentNumber*2)){//곱2 해본 경우가 포함 되면
					Set nextSet = new HashSet<Long>();
					
				}
				if()
			}
		}
	}
	private static void setData() throws NumberFormatException, IOException {
		N = Integer.parseInt(br.readLine());
		st = new StringTokenizer(br.readLine());
		seriesB = new long[N];
		for(int i=0;i<N;i++) seriesB[i] = Long.parseLong(st.nextToken());
		
	}
}
