	private static void topological() throws IOException {
		while(!q.isEmpty()){
			int zeroIndegree = q.poll();
			 for (int node : graph[zeroIndegree]) {
	                indegree[node]--;
	                if(indegree[node]==0){
	                    q.add(node);
	                    bw.write(node+" ");
	                }
	            }
		}
	}
