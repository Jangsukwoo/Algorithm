package CodingStudy;

import java.util.LinkedList;
import java.util.Scanner;

/*
 * N by N 
 * 5<=N<=25
 * 1로 인접한 영역은 한 영역으로 취급하고
 * 각 칸의 수가 가구의 수 
 * 각 영역의 가구의 수를 오름차순으로 출력하기
 * dfs로 풀어보기
 */
public class 단지번호붙이기 {
	static int[] dr = {-1,0,1,0};
	static int[] dc = {0,1,0,-1};//상우하좌
	static char[][] village;
	static boolean[][] areaVisit;
	static LinkedList<Integer> areaCount = new LinkedList<Integer>();
	static int N;
	static int cnt;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		village = new char[N][N];
		areaVisit = new boolean[N][N];
		sc.nextLine();
		for(int row=0;row<N;row++) {
			String readLine = sc.nextLine();
			village[row] = readLine.toCharArray();
		}

		//처리
		for(int row=0;row<N;row++) {
			for(int col=0;col<N;col++) {
				if(isNewArea(row,col)) {
					cnt=1;
					dfs(row,col);
					areaCount.add(cnt);
				}
			}
		}
	}

	private static void dfs(int row, int col) {
		for(int dir=0;dir<4;dir++) {
			int nr = row+dr[dir];
			int nc = col+dc[dir];
			if(!rangeCheck(nr,nc)) continue;
			if(isNewArea(nr,nc)) dfs(nr,nc);
		}
	}

	private static boolean rangeCheck(int nr, int nc) {
		if(nr>=0 && nr<N && nc>=0 && nc<N) return true;
		return false;
	}
	private static boolean isNewArea(int row, int col) {
		if(village[row][col]==1 && areaVisit[row][col]==false) {//새 영역이면
			areaVisit[row][col] = true;//방문처리
			cnt++;//방문카운트 ++
			return true;
		}
		return false;
	}
}
