package CodingStudy;
/*
 * N행 4열 
 */
public class 땅따먹기 {
	static int[][] dp;
	public static void main(String[] args) {

	}
    public int solution(int[][] land) {
    	int answer = 0;
    	dp = new int[land.length][3];
    	dp[0][0] = land[0][0];
    	dp[0][1] = land[0][1];
    	dp[0][2] = land[0][2];
    	dp[0][3] = land[0][3];
    	
        return answer;
    }
}
