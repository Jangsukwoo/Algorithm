package BAEKJOON;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

class VirusRC{
	int row;
	int col;
	public VirusRC(int r, int c){
		row = r;
		col = c;
	}
}
public class 연구소2 {
	static int N;
	static int M;
	static int time;
	static int second;
	static int possibleRClistSize;
	static int[] dr = {-1,0,1,0};
	static int[] dc = {0,1,0,-1}; //상우하좌
	static int[] virusCase;
	static int[][] area;
	static ArrayList<VirusRC> possibleRClist = new ArrayList<VirusRC>();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		area = new int[N][N];
		virusCase = new int[M];
		for(int row=0;row<N;row++) {
			for(int col=0;col<N;col++) {
				area[row][col] = sc.nextInt();
				if(area[row][col]==2) {
					possibleRClist.add(new VirusRC(row,col));
					area[row][col] = 0;
				}
			}	
		}
		
		possibleRClistSize = possibleRClist.size();
		
		nCr(0,0);
	}
	private static void nCr(int idx, int cnt){
		if(cnt==M){
			System.out.println(Arrays.toString(virusCase));
		}
		for(int num=idx;num<possibleRClistSize;num++){
			virusCase[cnt] = num;
			nCr(num+1,cnt+1);
		}
	}
}
