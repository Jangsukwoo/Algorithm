package 순열조합;

import java.util.Arrays;
import java.util.LinkedHashSet;
import java.util.Scanner;
/*
 * 링크드 해시셋 이용
 */
public class N과M9 {
	static int N,M;
	static int[] data;
	static int[] result;
	static boolean[] visit;
	static LinkedHashSet<String> linkedHashSet = new LinkedHashSet<String>();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		data = new int[N];
		visit = new boolean[N];
		result = new int[M];
		for(int i=0;i<N;i++) data[i] = sc.nextInt();
		Arrays.sort(data);//오름차순
		dfs(0);
		linkedHashSet.toString();
	}
	private static void dfs(int cnt) {
		if(cnt==M){
			String answer="";
			for(int i=0;i<M;i++){
				answer+=Integer.toString(result[i])+" ";
			}
			answer+="\n";
			linkedHashSet.add(answer);
		}
		for(int i=0;i<N;i++) {
			if(visit[i]==false){
				visit[i] = true;
				result[cnt] = data[i];
				dfs(cnt+1);
				visit[i]=false;
			}
		}
	}
}
