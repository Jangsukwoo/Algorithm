package DP;
/*
 * knapSack
 */
import java.util.Scanner; 
public class 수강과목{ 
	static Scanner sc=new Scanner(System.in); 
	public static void main(String[] args){ 
		int maxtime=sc.nextInt(); 
		int K=sc.nextInt(); 
		int[][] arr=new int[K+1][K+1]; 
		Subject[] subject=new Subject[K+1]; 
		for(int i=1;i<=K;i++) 
			subject[i]=new Subject(sc.nextInt(),sc.nextInt()); 
		for(int i=1;i<=K;i++){

			//1~K번째 보석까지 총 무게가 important가 되는 가치합의 최댓값 
			for(int j=1;j<=K;j++){ 
				if(subject[i].getimportant()<=j && arr[i-1][j]<=arr[i-1][j-subject[i].getimportant()]+subject[i].gettime()) 

					//i번째 보석의 무게가 j보다 작거나 같고,

					//이전 값이 이전 보석까지의 최적해에 i번째 가치를 더한값보다 작거나 같으면  

					arr[i][j]=arr[i-1][j-subject[i].getimportant()]+subject[i].gettime(); //이전 보석까지의 최적해에 i번째 가치를 더한다. 
				else //아닐 경우 이전 값을 유지한다. 
					arr[i][j]=arr[i-1][j]; 
			} 
		} 
		for(int i=1;i<=K;i++){ 
			for(int j=1;j<=important;j++) 
				System.out.print(arr[i][j]+" "); 
			System.out.println(); 
		} 
	} 
} 
class Subject{ //보석의 무게와 가격을 저장하기 위한 클래스 
	private int important; 
	private int time; 
	Subject(int important, int time){ 
		this.important=important; 
		this.time=time; 
	} 
	public int getimportant(){ 
		return important; 
	} 
	public int gettime(){ 
		return time; 
	} 
}