		B = sc.nextInt();
		C = sc.nextInt();
		int Y,X;
		while(true){
			if(A==B && A==C) {
				System.out.println("1");
				break;
			}else {
				if(stoneGroup[A][B][C]==false){
					if(A>=B){
						
					}
				}
			}
		}
	}
}
