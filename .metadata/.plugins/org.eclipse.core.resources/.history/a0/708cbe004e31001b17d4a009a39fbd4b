package CodingTest;

import java.util.HashMap;

public class 에스티6 {
	public static void main(String[] args) {

	}
    public String solution(int n) {
        String answer = "0";
        HashMap<Integer,Integer> fibonacci = new HashMap<Integer, Integer>();
        fibonacci.put(0,0);
        fibonacci.put(1,1);

        for(int num = 2;num<=70;num++) {
        	int nextValue = fibonacci.get(num-1)+fibonacci.get(num-2);
        	fibonacci.put(num,nextValue);
        }
        for(int num=-1;num>=-70;num--) {
        	int nextValue = fibonacci.get(num+2) - fibonacci.get(num+1);
        	fibonacci.put(num, nextValue);
        }
        answer = 
        return answer;
    }
}
