package BAEKJOON;

import java.util.Scanner;

public class 다음순열 {
	static int N;
	static int K = Integer.MAX_VALUE;
	static int[] targetData;
	static int[] data;
	static boolean[] visit;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		targetData = new int[N];
		data = new int[N];
		visit = new boolean[N];
		for(int i=0;i<N;i++) targetData[i] = sc.nextInt();
		dfs(0);
	}
	private static void dfs(int cnt) {
		if(cnt==N) {
			boolean find = true;
			for(int i=0;i<N;i++){
				if(data[i]!=targetData[i]) {
					find = false;
					break;
				}
			}
			return;
		}
		for(int i=0;i<N;i++){
			if(visit[i]==false){
				visit[i] = true;
				data[cnt] = (i+1);
				dfs(cnt+1);
				visit[i] = false;
			}
		}
	}
}
