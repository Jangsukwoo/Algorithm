package CodingStudy;

import java.util.Scanner;

public class 출근경로 {
	static int w,h;
	static int[][] city;
	static int path;
	static int[] dr = {0,1};
	static int[] dc = {1,0};//우,하
	public static void main(String[] args) {
		setData();
		getPath();
	}

	private static void getPath() {
		for(int dir=0;dir<2;dir++) {
			dfs(0,0,dir,false);//0,0에서 출발하고 교차 안꺾음			
		}
	}

	private static void dfs(int row, int col, int dir, boolean turn) {
		if(row==(h-1) && col==(w-1)) {
			
			return;
		}
		if(turn==false){//안꺾어봤으면
			if(dir==0){//오른쪽으로 가고있었으면
				dir = 1;//아래방향 한번 본다
				
			}
			else if(dir==1) {
				dir = 0;
			}
			
		}
	}

	private static void getPathDP() {
		for(int row=0;row<h;row++){//모든 path 구하기
			for(int col=0;col<w;col++){
				if(!(row==0 && col==1) && !(row==1&&col==0) && !(row==0 && col==0)){
					//처음 넣어준값 아니고 
					if(up(row-1) && left(col-1)){//윗방 왼쪽방 있으면
						city[row][col] = city[row-1][col]+city[row][col-1];
					}else if(up(row-1) && !left(col-1)){//윗방 있고 왼쪽방 없으면
						city[row][col] = city[row-1][col];
					}else if(!up(row-1) && left(col-1)) {//윗방 없고 왼쪽방 있으면
						city[row][col] = city[row][col-1];
					}
				}
			}
		}
	}

	private static boolean left(int nc) {
		if(nc>=0) return true;
		return false;
	}

	private static boolean up(int nr) {
		if(nr>=0) return true;
		return false;
	}

	private static void setData() {
		Scanner sc = new Scanner(System.in);
		w = sc.nextInt();
		h = sc.nextInt();
		city = new int[h][w];
		city[0][1] = 1;
		city[1][0] = 1;
	}
}
