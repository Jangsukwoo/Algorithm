package CodingTest;

public class 느흐느고도1 {
	public static void main(String[] args) {
		
	}
    public static int solution(int[] goods){
    	int sum = 0;
    	int discount = 0;
    	for(int i=0;i<goods.length;i++) sum+=goods[i];
    	discount = sum/50;
    	sum = sum - 10*discount;
        return sum;
    }
}
