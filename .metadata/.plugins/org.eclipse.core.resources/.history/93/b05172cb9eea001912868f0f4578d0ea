package BAEKJOON;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.StringTokenizer;

public class 나는야포켓몬마스터이다솜 {
	static int pokemons;
	static int questions;
	static HashMap<String,Integer> pokemonBookSearchName = new HashMap<String, Integer>();
	static HashMap<Integer,String> pokemonBookSearchNum = new HashMap<Integer, String>();
	static StringBuilder sb = new StringBuilder();
 	public static void main(String[] args) throws IOException {
 		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
 		StringTokenizer st = new StringTokenizer(br.readLine());
		pokemons = Integer.parseInt(st.nextToken());
		questions =Integer.parseInt(st.nextToken());

		for(int i=0;i<pokemons;i++) {
			String pokemon = br.readLine();
			pokemonBookSearchName.put(pokemon,i);
			pokemonBookSearchNum.put(i,pokemon);
		}
		for(int q=0;q<questions;q++) {
			String question = br.readLine();
			char firstData = question.charAt(0);
			if(firstData>='0' && firstData <='9'){//숫자면
				int pokemonNum = Integer.parseInt(question);
				sb.append(pokemonBookSearchNum.get(pokemonNum)+"\n");			
			}else{//문자면
				int size = pokemonList.size();
				for(int p=0;p<size;p++){
					if(pokemonList.get(p).equals(question)){
						sb.append((p+1)+"\n");
						break;
					}
				}
			}
		}
		System.out.println(sb.toString());
	}
}
