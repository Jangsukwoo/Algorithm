package Simulation;

import java.util.Scanner;

public class 체스판다시칠하기 {
	static int N,M;//R,C size
	static char[][] chessBoard;
	static int minSquareValue = Integer.MAX_VALUE;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int M = sc.nextInt();
		chessBoard = new char[N][M];
		sc.nextLine();
		for(int row=0;row<N;row++) {
			String readLine = sc.nextLine();
			chessBoard[row] = readLine.toCharArray();
		}
		for(int row=0;(row+7)<N;row++) {
			for(int col=0;(col+7)<M;col++){
				sliceBoardCheck(row,col);
			}
		}
		System.out.println(minSquareValue);
	}
	private static void sliceBoardCheck(int row, int col) {
		switch (chessBoard[row][col]){
		case 'W':			
			break;
		case 'B':
			break;
		}
	}
}
