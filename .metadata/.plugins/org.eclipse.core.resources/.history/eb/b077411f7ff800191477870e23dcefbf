package BAEKJOON;

import java.util.Scanner;

public class 다음순열 {
	static int N;
	static int K = Integer.MAX_VALUE;
	static int[] targetData;
	static int[] data;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		targetData = new int[N];
		data = new int[N];
	}
}
