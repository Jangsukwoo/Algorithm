package 브루트포스;

import java.util.Scanner;

/*
 * 한수?
 * 어떤 양의 정수 X의 자리수가 등차수열인 것을 말함
 * 입력값 N에 대해
 * 1보다 크거나 같고 N보다 작거나 같은 한수의 개수 출력
 * N Size <= 1000 
 */
public class 한수 {
	static int N;
	static int answer;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		for(int i=1;i<=N;i++){
			boolean flag = false;
			if(i>10) {
				String series = Integer.toString(i);
				
			}
			else answer++;
			
		}
	}
}
