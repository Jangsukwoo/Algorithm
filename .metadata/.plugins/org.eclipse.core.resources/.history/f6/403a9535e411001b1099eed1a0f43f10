package 구현;

import java.util.Scanner;

public class 지구온난화 {
	static int R,C;
	static char[][] map;
	static char[][] newMap;
	public static void main(String[] args) {
		Scanner sc = new  Scanner(System.in);
		R = sc.nextInt();
		C= sc.nextInt();
		newMap= new char[R][C];
		sc.nextLine();
		
		for(int row=0;row<R;row++) map[row] = sc.nextLine().toCharArray();
		
		getAfter50YearsMap();
	}
}
