package CodingStudy;

import java.util.Scanner;

/*
 * 
 */
public class 모의고사 {
	static int[] first;
	static int[] second;
	static int[] third;
	public static void main(String[] args) {

	}
	public int[] solution(int[] answers) {
		int[] answer = {};
		int size = answers.length;
		makeSeries(size);
		return answer;
	}
	private void makeSeries(int size) {
		first = new int[size];
		second = new int[size];
		third = new int[size];
		for(int i=1;i<=size;i++) {
			if(i%5==0) {
				first[i-1]=5;
			}else first[i-1]=i%5;
		}
	}
}
