package CodingTest;

public class 느흐느고도1 {
	static int minCost;
	public static void main(String[] args) {
		
	}
    public static int solution(int[] goods){
    	minCost = Integer.MAX_VALUE;
    	int sum = 0;
    	for(int i = 0; i<=3; i++) { //하나씩 구매해보기
    		sum+=goods[i];
    		if(goods[i]>=50) {
    			sum-=10;
    		}
    	}
    	minCost = Math.min(minCost,sum);
    	sum = 0;
    	for(int i=0;i<3;i++) {
    		for(int j=i+1;j<3;j++) {
    			
    		}
    	}
    	return 0;
    }
}
