package DP;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;
/*
 * 14:25~
 */
public class 태상이의훈련소생활 {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static int N,M;
	static int[] heights;
	static int[] segmentTree;
	public static void main(String[] args) throws IOException {
		setData();
	}
	private static void setData() throws IOException {
		st = new StringTokenizer(br.readLine());
		N = Integer.parseInt(st.nextToken());
		M = Integer.parseInt(st.nextToken());
		segmentTree = new int[N*4+1];
		heights = new int[N+1];
		st = new StringTokenizer(br.readLine());
		for(int num = 1 ; num<=N; num++) heights[num] = Integer.parseInt(st.nextToken());
		setSegmentTree(1,1,N); //세그트리 구성
		for(int i=0;i<M;i++) {
			
		}
	}
	private static int setSegmentTree(int node, int start, int end){
		if(start==end) return segmentTree[node] = heights[start]; //스타트가 아니라 node임 
		int mid = (start+end)/2;
		return segmentTree[node] = setSegmentTree(node*2, start, mid) + setSegmentTree((node*2)+1,mid+1, end);
	}

}
