package Samsung;

import java.util.Scanner;

/*
 * 4:25~
 * 번호가 Xi의 배수인 원판을
 * di 방향으로
 * ki칸 회전
 * di = 0 : 시계방향
 * di = 1: 반시계방향
 * 
 */
public class 기출_원판돌리기 {
	static int N,M,T;//원판 N개, 원판에 적힌 숫자 M개, 회전 T번
	static int[][] circleBoard;
	static int x,d,k;
	//x의 배수인 원판들을 대상으로
	//d 방향 ( 0: 시계 , 1 : 반시계)
	//k 칸만큼 회전
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		T = sc.nextInt();
		circleBoard = new int[N][M];
		for(int i=0;i<N;i++)
			for(int j=0;j<M;j++) circleBoard[i][j] = sc.nextInt();
		
		x = sc.nextInt();
		d = sc.nextInt();
		k = sc.nextInt();
	}
}
