package 구현;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.Stack;
import java.util.StringTokenizer;

public class 피자굽기 {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static int D,N;
	static Stack<Long> stack = new Stack<Long>();
	static long[] dough;
	static long[] oven;
	static int doughIdx;
	public static void main(String[] args) throws IOException {
		setData();
		setStack();
		getAnswer();
	}
	private static void getAnswer() {
		
	}
	private static void setStack() {
		for(int i=0;i<D;i++) {
			if(oven[i]<=dough[doughIdx]) stack.add(oven[i]);
			else {
				doughIdx++;
				break;
			}
		}
	}
	private static void setData() throws IOException{
		st = new StringTokenizer(br.readLine());
		D = Integer.parseInt(st.nextToken());
		N = Integer.parseInt(st.nextToken());
		oven = new long[D];
		dough = new long[N];
		st = new StringTokenizer(br.readLine());
		for(int i=0;i<D;i++) oven[i] = Long.parseLong(st.nextToken());
		st = new StringTokenizer(br.readLine());
		for(int i=0;i<N;i++) dough[i] = Long.parseLong(st.nextToken());
	}
}
