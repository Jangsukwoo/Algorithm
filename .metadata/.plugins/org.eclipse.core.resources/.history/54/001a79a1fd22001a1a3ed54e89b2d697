package winterSDS;

import java.util.Scanner;
/*
 * 선분 수 N
 * 1<=N<=200000
 * 선분의 좌표 X,Y
 * 0<=X,Y<=10억
 * 
 * 왕자와 공주는 각각 자신의 경로로 매일 한칸 씩 순시를 함
 * 최대 1000000일 순시
 * 
 * 동일한 위치를 다시 지나가는 것도 가능
 * 
 * 메신저는 왕자의 현재 위치에서 공주의 순시 경로 위에 위치시키는데
 * 메신저를 위치 시키는 조건은
 * 메신저의 출발위치와 X 또는 Y 좌표가 동일한 위치에만 이동할 수 있다.
 * 
3 //T
3 //N
1 6 //왕자 N개
2 5
2 2
1 1
6 1
6 2
3 5
4 6
1
1 1
1 5
5 1
5 3
1
1 1
1 5
2 6
9 6
 */
public class FastMessaging {
	static int N; 
	static long[][] princeXY;
	static long[][] princessXY;
	static int day;
	public static void main(String[] args) {
		Scanner sc  = new Scanner(System.in);
		int T = sc.nextInt();
		for(int testcase=1;testcase<=T;testcase++) {
			N = sc.nextInt();
			princeXY = new long[N+1][2];
			princessXY = new long[N+1][2];
			for(int i=0;i<(N+1);i++) {
				princeXY[i][0] = sc.nextLong();
				princeXY[i][1] = sc.nextLong();
			}
			for(int i=0;i<(N+1);i++) {
				princessXY[i][0] = sc.nextLong();
				princessXY[i][1] = sc.nextLong();
			}//입력
		}
	}
}
