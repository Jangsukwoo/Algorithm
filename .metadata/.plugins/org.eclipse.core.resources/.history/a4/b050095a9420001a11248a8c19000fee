package CodingStudy;

import java.util.Scanner;

/*
 * N by N 
 * 5<=N<=25
 * 1로 인접한 영역은 한 영역으로 취급하고
 * 각 칸의 수가 가구의 수 
 */
public class 단지번호붙이기 {
	static char[][] village;
	static int N;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		village = new char[N][N];
	}
}
