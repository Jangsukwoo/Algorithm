package 탐색;

import java.util.Arrays;

public class 입국심사 {
	/*
	 */
	public static void main(String[] args) {
		long answer = solution(6,new int[] {7,10});
		System.out.println(answer);
	}
    public static long solution(int n, int[] times) {
        long answer = 0;
        Arrays.sort(times);
        long startTime = 1;
        long endTime = Integer.toUnsignedLong(times[times.length-1])*Integer.toUnsignedLong(n);
        while(startTime<=endTime){
        	int peopleCount = getPeopleCount(times,endTime);
        }
        return answer;
    }
	private static int getPeopleCount(int[] times, long endTime) {
		int peopleCount = 0;
		for(int time : times) {
			peopleCount+=(endTime/time);
		}
		return 0;
	}
}
