package CodingTest;

public class 느흐느고도1 {
	static int minCost;
	public static void main(String[] args) {
		
	}
    public static int solution(int[] goods){
    	minCost = Integer.MAX_VALUE;
    	int sum = 0;
    	for(int choose = 1; choose<=3; choose++) { //하나씩 구매해보기
    		sum+=goods[choose];
    		if(goods[choose]>=50) {
    			sum-=10;
    		}
    	}
    	minCost = Math.min(minCost,sum);
    	return 0;
    }
}
