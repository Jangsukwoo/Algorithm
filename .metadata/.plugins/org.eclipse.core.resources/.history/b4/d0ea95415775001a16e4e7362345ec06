package CodingStudy;

import java.util.Arrays;
import java.util.Scanner;

/*
 * 약이 N개 담긴 병
 * 한 조각을 꺼낸 날은 W
 * 반 조각을 꺼낸 날은 H
 * 2N일이 지나면 길이가 2N인 문자열이 만들어짐
 * 이때 서로 다른 2N 길이의 문자열의 개수는?
3814986502092304
9223372036854775807 //long 최대 
 */
public class 알약 {
	static int N;
	static long[] answer = new long[31];
	static long[][] memo = new long[31][31];
	static long cnt;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		setDrugMemo();
		while(true){
			N = sc.nextInt();
			if(N==0) return;
			else System.out.println(memo[N]);
		}
	}
	private static void setDrugMemo() {
		for(int drug=1;drug<=10;drug++){
			cnt=0;
			recursive(drug,0);
			answer[drug] = cnt;
		}
		System.out.println(Arrays.toString(answer));
	}
	private static void recursive(int w, int h) {
		if(w==0) cnt++;
		else{//먹을 알약이 있는 경우
			recursive(w-1,h+1);//한조각--, 반조각++
			if(h>0) recursive(w, h-1); //반조각 --, 한조각 그대로
		}
	}
}
