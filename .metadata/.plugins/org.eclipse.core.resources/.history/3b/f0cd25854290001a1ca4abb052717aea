import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.StringTokenizer;

/*
 * 18:00
 * 진수가 적어도 K명의 병사를 이길 수 있는
 * 최소의 스탯포인트
 */
public class 용감한용사진수 {
	static int N,K;//병사수, 이겨야할 수
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static ArrayList<Enemy> enemylist;
	static class Jinsu{
		int power;
		int dex;
		int intelligence;
		public Jinsu(int power, int dex, int intelligence) {
			this.power = power;
			this.dex = dex;
			this.intelligence = intelligence;
		}
	}
	static class Enemy{
		int power;
		int dex;
		int intelligence;
		int allStat;
		public Enemy(int power, int dex, int intelligence, int allStat) {
			super();
			this.power = power;
			this.dex = dex;
			this.intelligence = intelligence;
			this.allStat = allStat;
		}
	}
	public static void main(String[] args) {
		setData();
	}
	private static void setData(){
		
	}
}
