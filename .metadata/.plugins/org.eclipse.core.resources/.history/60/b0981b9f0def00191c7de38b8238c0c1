package Samsung;
import java.util.ArrayList;
/*
 * 시작 200
 * 
 */
import java.util.Scanner;
class Pinball{
	int row;
	int col;
	int dir;
	public Pinball(int r, int c, int d){
		row = r;
		col = c;
		dir =d;
	}
}
public class 모의_핀볼게임 {
	static int[] dr = {-1,0,1,0};
	static int[] dc = {0,1,0,-1};//상우하좌
	static int N;
	static int[][] board;
	static boolean[][][] visitAnddirection;
	static int score;
	static int startR, startC;
	static ArrayList<Pinball> pinballList = new ArrayList<Pinball>();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int testcase=1;testcase<=T;testcase++){
			N = sc.nextInt();
			board = new int[N][N];
			visitAnddirection = new boolean[N][N][4];
			pinballList.clear();
			for(int row=0;row<N;row++) {
				for(int col=0;col<N;col++) {
					board[row][col] = sc.nextInt();
					if(board[row][col]==0) pinballList.add(new Pinball(row,col,0));
				}
			}
			
			int size = pinballList.size();
			
			for(int i=0;i<size;i++){//시작
				
				for(int dir=0;dir<4;dir++) {
					Pinball curPinball = pinballList.get(i);
					startR = curPinball.row;
					startC = curPinball.col;
					curPinball.dir=dir;
					dfs(curPinball);
				}
			}
						
		}
	}
	private static void dfs(Pinball curPinball) {
		if(board[curPinball.row][curPinball.col]==-1 || (curPinball.row==startR && curPinball.col==startC)){
			
		}
	}
}
