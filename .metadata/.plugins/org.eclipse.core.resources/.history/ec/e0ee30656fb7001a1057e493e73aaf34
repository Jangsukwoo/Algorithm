package 알골자구공부;

public class 정렬 {
	public static void main(String[] args) {
		int[] data = new int[] {5,4,3,2,1};
		bubbleSort(data);
	}

	/*
	 * 버블정렬
	 */
	private static void bubbleSort(int[] data) {
		for(int i=0;i<data.length;i++) {
			for(int j=0;j<(data.length-1);j++) {
				if(data[j]>data[j+1]){//swap
					int temp = data[j];
					data[j] = data[j+1];
					data[j+1] = temp;
				}
			}
		}
	}
}
