package CodingStudy;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.StringTokenizer;

/*
 * 징검다리들을 노드화 하고 다익스트라 돌리자 
 */
public class 알고스팟 {
	static int N,F;
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static Vertax[] vertaxs;
	static double[] dist;
	static ArrayList<int[]>[] adjList;//인접리스트
	static class Vertax{
		int num;
		int x;
		int y;
		public Vertax(int num, int x, int y) {
			this.num = num;
			this.x = x;
			this.y = y;
		}
	}
	public static void main(String[] args) throws IOException {
		setData();
		makeAdjList();
	}
	private static void makeAdjList() {
		
	}
	private static void setData() throws IOException {
		st = new StringTokenizer(br.readLine());
		N = Integer.parseInt(st.nextToken());
		F = Integer.parseInt(st.nextToken());
		vertaxs = new Vertax[N+2];
		dist = new double[N+2];
		adjList = new ArrayList[N+2];
		for(int i=1;i<=N+1;i++) adjList[i] = new ArrayList<int[]>();
		vertaxs[1] = new Vertax(1,0,0);//시작 위치
		for(int no=2,x,y;no<=N+1;no++){
			st = new StringTokenizer(br.readLine());
			x = Integer.parseInt(st.nextToken());
			y = Integer.parseInt(st.nextToken());
			vertaxs[no] = new Vertax(no, x, y);
		}
	}
}
