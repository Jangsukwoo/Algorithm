package BAEKJOON;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;
import java.util.StringTokenizer;

public class 숫자의신 {
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static int N,K;
	static int max;
	static ArrayList<String> numlist;
	static StringBuilder sb = new StringBuilder();
	public static void main(String[] args) throws IOException {
		setData();
		getMaxValue();
		
	}
	private static void getMaxValue() {
		for(int i=0;i<K;i++) {
			String value = numlist.get(i);
			sb.append(value);
			if(max==Integer.parseInt(value)){
				for(int j=0;j<(N-K);j++) {
					sb.append(value);
				}
			}
		}
		for(int i=0;i<(N-K);i++) {
			
		}
	}
	private static void setData() throws IOException {
		st = new StringTokenizer(br.readLine());
		K = Integer.parseInt(st.nextToken());
		N = Integer.parseInt(st.nextToken());
		numlist =  new ArrayList<String>();
		for(int i=0;i<K;i++) {
			String sv = br.readLine();
			int value = Integer.parseInt(sv);
			if(max<value) max = value;
			numlist.add(sv);
		}
		Collections.sort(numlist,new Comparator<String>() {
			@Override
			public int compare(String o1, String o2) {
				String o=o1+o2;
				return -o.compareTo(o2+o1);
			}
		});
	}

}
