package BAEKJOON;

import java.util.ArrayList;
import java.util.Scanner;

public class 나는야포켓몬마스터이다솜 {
	static int pokemons;
	static int questions;
	static ArrayList<String> pokemonList = new ArrayList<String>();
	static StringBuilder sb = new StringBuilder();
 	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		pokemons = sc.nextInt();
		questions = sc.nextInt();
		sc.nextLine();
		for(int i=0;i<pokemons;i++) {
			String pokemon = sc.nextLine();
			pokemonList.add(pokemon);
		}
		for(int q=0;q<questions;q++) {
			String question = sc.nextLine();
			char firstData = question.charAt(0);
			if(firstData>='0' && firstData <='9'){//숫자면
				int pokemonNum = Integer.parseInt(question);
				sb.append(pokemonList.get((pokemonNum-1));
			}else{//문자면
				int size = pokemonList.size();
				for(int p=0;p<size;p++){
					if(pokemonList.get(p).equals(question)){
						System.out.println((p+1));
						break;
					}
				}
			}
		}
	}
}
