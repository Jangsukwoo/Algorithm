import java.util.Scanner;
class Pipe{
	int headRow;
	int headCol;
	int tailRow;
	int tailCol;

	public Pipe(int hr,int hc, int tr, int tc) {
		headRow = hr;
		headCol = hc;
		tailRow = tr;
		tailCol = tc;
	}
}
public class 상시_파이프옮기기 {
	static int N;
	static int[][] house;
	static int[] dr = {0,1,1};
	static int[] dc = {1,1,0}; // 우,우하,하
	static int cnt;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		house = new int[N][N];
		for(int row=0;row<N;row++)
			for(int col=0;col<N;col++) house[row][col] = sc.nextInt();
		Pipe curpipe= new Pipe(0,1,0,0);
		dfs(curpipe);
	}
}
