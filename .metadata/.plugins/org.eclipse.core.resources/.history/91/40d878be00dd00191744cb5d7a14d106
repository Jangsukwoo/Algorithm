package CodingTest;

import java.util.ArrayList;

public class mailBox {
	static ArrayList<String> tempBox;
	static ArrayList<String> mailBox;
	public static void main(String[] args) {
		String[] record = {"RECEIVE abcd@naver.com",
		                   "RECEIVE zzkn@naver.com",
		                   "DELETE", 
		                   "RECEIVE qwerty@naver.com",
		                   "SAVE", 
		                   "RECEIVE QwerTY@naver.com"};
		String email;
		for(int r=0;r<record.length;r++) {
			if(record[r].contains("RECEIVE")){
				email = record[r].substring(8);
				tempBox.add(email);
			}
			else if(record[r].contains("SAVE")) {
				for(int i=0;i<tempBox.size();i++) {
					mailBox.add(tempBox.get(i));
				}
			}
			else if(record[r].contains("DELETE")) {
				
			}
		}
	}
}
