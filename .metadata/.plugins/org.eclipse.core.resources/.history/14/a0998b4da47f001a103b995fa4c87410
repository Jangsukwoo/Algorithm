		int x;
		int y;
		public Vertax(int x, int y) {
			this.x = x;
			this.y = y;
		}		
	}
	static class Hole{
		int leftX;
		int leftY;
		int rightX;
		int rightY;
		public Hole(int leftX, int leftY, int rightX, int rightY) {
			super();
			this.leftX = leftX;
			this.leftY = leftY;
			this.rightX = rightX;
			this.rightY = rightY;
		}
	}
	public static void main(String[] args) throws NumberFormatException, IOException {
		setData();
	}
