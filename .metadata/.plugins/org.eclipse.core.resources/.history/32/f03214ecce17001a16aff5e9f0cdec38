package CodingStudy;
/*
 * String인  numbers 문자의 최대 사이즈 7
 * 17,71 처럼 순서가 있으니 Permutation nPr 
 */
public class 소수찾기 {
	static int prime;
	static int n,r;
	static boolean[] visit;
	static char[] number;
	static char[] data;
	public static void main(String[] args) {
		int N = Integer.parseInt("011");
	}
    public int solution(String numbers) {
        int answer = 0;
        data = numbers.toCharArray();
        int n = numbers.length();
        for(int i=1;i<=n;i++){//nPr
        	visit = new boolean[n];
        	number = new char[i];
        	r = i;
        	permutation(0);
        }
        return answer;
    }
	private void permutation(int cnt) {
		if(cnt==r){
			return;
		}
		for(int i=0;i<n;i++){
			if(visit[i]==false){
				number[cnt] = data[i];
			}
		}
	}
}
