package CodingStudy;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;

/*
 * n보다 크고 2n보다 작거나 같은 소수의 개수
 * 
 */
public class 베르트랑공준 {
	static StringBuilder sb = new StringBuilder();
	static boolean[] Eratosthenes = new boolean[250001];
	static ArrayList<Integer> primeList = new ArrayList<Integer>();
	public static void main(String[] args) throws IOException{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		makeEratosthenes();
		while(true){
			String readLine = br.readLine();
			int N = Integer.parseInt(readLine);
			if(N==0) break;
			int i=1;
			int cnt=0;
			while(true){
				if(N<primeList.get(i) && primeList.get(i)<=(N*2)) {
					cnt++;
				}
				if(primeList.get(i)>(N*2)) break;
			}
		}
		
	}
	private static void makeEratosthenes() {
		for(int i=2;i<=250000;i++){
			if(Eratosthenes[i]) continue; //소수면 무시
			for(int j=i+i;j<=250000;j+=i){
				Eratosthenes[j] = true;//배수는 전부 true
			}
		}
		for(int i=1;i<=250000;i++) {
			if(Eratosthenes[i]==false) primeList.add(i);
		}
	}
}
