package DP;

import java.util.Scanner;
//L I S
//최장증가부분수열 
//O(n^2) 풀이와 O(nlogn)풀이가 존재


public class 반도체설계 {
	static int N;
	static int[] port;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		port = new int[N+1];
		for(int i=1;i<=N;i++) port[i] = sc.nextInt();
		
	}
}
