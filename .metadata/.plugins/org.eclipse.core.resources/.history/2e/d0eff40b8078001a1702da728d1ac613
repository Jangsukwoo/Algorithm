package CodingStudyHW;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

/*
 * 11:54 시작
 * 돌 그룹 A,B,C가 있음
 * 크기가 같지 않은 두 그룹 고르고
 * 작은쪽을 X, 큰쪽을 Y라 함
 * X엔 X+X
 * Y엔 Y-X
 * 
 * 돌을 같은 개수로 만들 수 있으면 1 아니면 0 
 */
public class 돌그룹 {
	static boolean[][][] stoneGroup;
	static int A,B,C;
	static int result;
	static boolean find;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		stoneGroup = new boolean[501][501][501];
		bfs();
		if(find) System.out.println("1");
		else System.out.println("0");
	}
	private static void bfs() {
		int[] data = new int[]{A,B,C};
		Queue<int[]> q = new LinkedList<int[]>();
		stoneGroup[A][B][C] = true;
		q.add(data);
		while(!q.isEmpty()){
			int[] currentABC = q.poll();
			int a = currentABC[0];
			int b = currentABC[1];
			int c = currentABC[2];
			System.out.println(a+" "+b+" "+c);
			if(stoneGroup[a][b][c]==false) {
			if(a==b && a==c) {
				find = true;
				return;
			}
			if(a!=b){
				if(a>b){
					a-=b;
					b+=b;
				}
				else {
					b-=a;
					a+=a;
				}
				if(stoneGroup[a][b][c]==false) {
					stoneGroup[a][b][c]=true;
					q.add(new int[] {a,b,c});
				}
			}
			if(b!=c){
				if(b>c){
					b-=c;
					c+=c;
				}
				else {
					c-=b;
					b+=b;
				}
				if(stoneGroup[a][b][c]==false){
					stoneGroup[a][b][c]=true;
				}
					q.add(new int[] {a,b,c});
				}
			}
			if(a!=c){
				if(a>c){
					a-=c;
					c+=c;
				}
				else {
					c-=a;
					a+=a;
				}
				if(stoneGroup[a][b][c]==false) {
					stoneGroup[a][b][c]=true;
					q.add(new int[] {a,b,c});
				}
			}
		}
	}
}
