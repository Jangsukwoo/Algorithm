package CodingStudyHW;

import java.util.Scanner;

/*
 * 나무는 1,2 두그루가 있다.
 * 자두가 떨어질 때 까지 기다림
 * 자두는 1<=T<=1000초 동안 떨어짐
 * 자두는 최대 1<=W<=30만큼 움직이고싶음
 * 자두가 받을 수 있는 최대 자두 수?
 * 자두는 1번 자두 나무 위치에 있다고 한다.
 * 
 * 1. 배열...memo[2][w]?
 * -> memo[T][W]
 * t초에 w만큼 움직였을 때 얻을 수 있는 최대 자두 개수 
 * 2. 
 */
public class 자두나무 {
	static int T;
	static int W;
	static int[] postion;
	static int[][] memo;
	public static void main(String[] args) {
		setData();
		memoization();
	}
	private static void memoization() {
		for()
	}
	private static void setData() {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		W = sc.nextInt();
		postion = new int[T];
		memo = new int[T][W];
		for(int i=0;i<T;i++) postion[i] = sc.nextInt();
	}
}
