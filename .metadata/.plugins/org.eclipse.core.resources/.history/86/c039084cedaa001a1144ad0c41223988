package CodingAcademy;

public class 부분집합구하기 {
	static int[] set = new int[] {1,2,3};
	static int answer;
	static int[] pick;
	public static void main(String[] args){
		pick = new int[2];
		System.out.println(numSubsets(3,2));
	}
	private static int numSubsets(int n, int k) {
		if(k==0) {
			
			return 1;
		}
		numSubsets(n, k-1);
		return 0;
		
	}
}
