package Samsung;

import java.util.Scanner;

/* 시작
 * 모든 1을 덮는데 필요한 종이의 최소 개수
 * 
 */
public class 상시_색종이붙이기 {
	static int[][] paper;
	static int[] papersize = new int[6];
	static boolean[][] painted;
	static int startR,startC;
	static int min = Integer.MAX_VALUE;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		paper = new int[10][10];
		painted = new boolean[10][10];
		for(int i=1;i<=5;i++) papersize[i] = 5;
		for(int row=0;row<10;row++)
			for(int col=0;col<10;col++) paper[row][col] = sc.nextInt();
		dfs(0);
		if(min==Integer.MAX_VALUE) System.out.println("-1");
		else System.out.println(min);
	}
	private static void dfs(int use){
		if(paperCheck()){
			min = Math.min(min,use);
			return ;
		}
		//getStartRC();
		for(int p=5;p>=1;p--){
			if(papersize[p]>0){
				if(possible(startR,startC,p)){
					attach(startR,startC,p);
					view();
					papersize[p]-=1;
					dfs(use+1);
					detach(startR,startC,p);
					view();
					papersize[p]+=1;
				}
			}
		}
	}

private static void view() {
		for(int row=0;row<10;row++) {
			for(int col=0;col<10;col++) {
				System.out.print(painted[row][col]+" ");
			}
			System.out.println();
		}
		System.out.println();
	}
//	private static void getStartRC() {
//		for(int row=0;row<10;row++) {
//			for(int col=0;col<10;col++) {
//				if(paper[row][col]==1 && painted[row][col]==false) {
//					startR = row;
//					startC = col;
//					return;
//				}
//			}
//		}
//	}
	private static void detach(int sR, int sC, int p) {
		for(int row = (sR+p-1);row>=sR;row--) {
			for(int col=(sC+p-1);col>=sC;col--){
				painted[row][col]=false;
			}
		}
	}
	private static boolean possible(int sR, int sC, int p){
		if((sR+p)>=10 || (sC+p)>=10) return false;
		for(int row = sR;row<(sR+p);row++) {
			for(int col=sC;col<(sC+p);col++){
				if(paper[row][col]!=1) return false;
			}
		}
		return true;
	}
	private static void attach(int sR, int sC, int p) {
		for(int row = sR;row<(sR+p);row++) {
			for(int col=sC;col<(sC+p);col++){
				painted[row][col]=true;
			}
		}
	}
	private static boolean paperCheck() {
		for(int row=0;row<10;row++) {
			for(int col=0;col<10;col++) {
				if(paper[row][col]==1 && painted[row][col]==true) continue;
				else if(paper[row][col]==1 && painted[row][col]==false) {
					startR = row;
					startC = col;
					return false;
				}
			}
		}
		return true;
	}
}
