package Samsung;

/*
 * 시작 415
 * 
 */
import java.util.Scanner;
class Pipe{
	int headRow;
	int headCol;
	int tailRow;
	int tailCol;
	int set;
	public Pipe(int hr,int hc, int tr, int tc, int s) {
		headRow = hr;
		headCol = hc;
		tailRow = tr;
		tailCol = tc;
		set = s;
	}
}
public class 상시_파이프옮기기 {
	static int N;
	static int[][] house;
	static int[] dr = {0,1,1};
	static int[] dc = {1,1,0}; // 우,우하,하
	static int cnt;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		house = new int[N][N];
		for(int row=0;row<N;row++)
			for(int col=0;col<N;col++) house[row][col] = sc.nextInt();
		Pipe curpipe= new Pipe(0,1,0,0,0);
		dfs(curpipe);
	}
	private static void dfs(Pipe curpipe){
		if(curpipe.headRow==(N-1)&& curpipe.headCol==(N-1)){//도착
			cnt++;
			return;
		}
		switch (curpipe.set){
		case 0:
			int nhr = curpipe.headRow +dr[1];
			int nhc = curpipe.headCol +dc[1];
			int ntr = curpipe.tailRow +dr[1];
			int ntc = curpipe.tailCol +dc[1];
			if(rangeCheck(nhr,nhc)){//일단 가보려고 하는 장소가 만족하고
				if(possibleRC(nhr,nhc) && possibleRC(curpipe.tailRow+1,curpipe.headCol) && possibleRC(curpipe.headRow,curpipe.headCol+1)){
					//놓을 수 있다면
					dfs(new Pipe(nhr,nhc, ntr, ntc,2));
				}
			}
			int nhr = curpipe.headRow +dr[0];
			int nhc = curpipe.headCol +dc[0];
			int ntr = curpipe.tailRow +dr[0];
			int ntc = curpipe.tailCol +dc[0];
			break;
		case 1:
			break;
		case 2:
			break;
		}	
	}
	private static boolean possibleRC(int r, int c) {
		if(house[r][c]!=1) return true;
		return false;
	}
	private static boolean rangeCheck(int nhr, int nhc){
		if(nhr>=0 && nhr<N && nhc>=0 && nhc<N) return true;
		return false;
	}
	
}
