package winterSDS;

import java.util.Scanner;
/*
 * 선분 수 N
 * 1<=N<=200000
 * 선분의 좌표 X,Y
 * 0<=X,Y<=10억
 * 
 * 왕자와 공주는 각각 자신의 경로로 매일 한칸 씩 순시를 함
 * 
 * 동일한 위치를 다시 지나가는 것도 가능
 * 
 * 메신저는 왕자의 현재 위치에서 공주의 순시 경로 위에 위치시키는데
 * 메신저를 위치 시키는 조건은
 * 메신저의 출발위치와 X 또는 Y 좌표가 동일한 위치에만 이동할 수 있다.
 * 
3 //T
3 //N
1 6 //왕자 N개
2 5
2 2
1 1
6 1
6 2
3 5
4 6
1
1 1
1 5
5 1
5 3
1
1 1
1 5
2 6
9 6
 */
public class FastMessaging {
	static int N; //좌표 개수
	static long[] princeX;
	static long[] princeY;
	static long[] princessX;
	static long[] princessY;
	static int answer;
	public static void main(String[] args) {
		Scanner sc  = new Scanner(System.in);
		int T = sc.nextInt();
		for(int testcase=1;testcase<=T;testcase++) {
			N = sc.nextInt();
			princeX = new long[N+1];
			princeY = new long[N+1];
			princessX = new long[N+1];
			princessY = new long[N+1];
			for(int i=0;i<(N+1);i++) {
				princeX[i] = sc.nextLong();
				princeY[i] = sc.nextLong(); //왕자의 좌표
			}
			for(int i=0;i<(N+1);i++) {
				princessX[i] = sc.nextLong();
				princessY[i] = sc.nextLong();//공주의 좌표
			}//입력
			move();//처리
		}
	}
	private static void move() {

	}
}
