package CodingStudy;

import java.util.Scanner;

/*
 * 최장증가부분수열
 * dp 배열에 메모이제이션하면서
 * 가장 앞쪽 자리부터 구해나간다.  
 * 구하면서 각 자리의 최대 길이를 메모
 */
public class 상자넣기 {
	static int N;
	static int[] dp;
	static int[] box; //수열이라고 생각
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		box = new int[N];
		dp = new int[N];
		dp[0] = 1;
		for(int i=0;i<N;i++){//기준이 되는 박스
			int check=0;//기준 값보다 작으면서 체크 시점의 최대값
			for(int j=0;j<i;j++){//맨 앞 박스부터 기준값까지 스캔해보다가
				if(box[j]<=box[i]){//기준값보다 작으면서
					if(dp[j]>check){
						check = dp[j];
						dp[i]=dp[j]+1;
					}
				}
			}
		}
	}
}
