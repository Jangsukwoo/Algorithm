package CodingStudyHW;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

/*
 * 14:25~
 * N,N까지 이동시키는 방법의 수?
 * 빈칸은 0 ,벽은 1
 * 0,1에서 시작하고
 * 우, 우하, 하
 * 
 */
public class 파이프옮기기2{
	static int N;
	static int[][] house;
	static long[][] memo;
	static long answer;
	static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	static StringTokenizer st;
	static class Pipe{
		int row;
		int col;
		char state;
		public Pipe(int row, int col, char state) {
			this.row = row;
			this.col = col;
			this.state = state;
		}
	}
	public static void main(String[] args) throws IOException {
		setData();
		answer = dfs(new Pipe(0,1,'-'));
	}
	private static long dfs(Pipe currentPipe){
		if(currentPipe.row==(N-1) && currentPipe.col==(N-1)) {
			
			return 1;
		}
		
		memo
		return 0;
	}
	private static void setData() throws IOException {
		st = new StringTokenizer(br.readLine());
		N = Integer.parseInt(br.readLine());
		house = new int[N][N];
		memo = new long[N][N];
		for(int row=0;row<N;row++) {
			st = new StringTokenizer(br.readLine());
			for(int col=0;col<N;col++) {
				house[row][col] = Integer.parseInt(st.nextToken());
			}
		}
	}
}
