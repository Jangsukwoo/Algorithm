package BAEKJOON;

import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class 치즈 {
	static Queue<int[]> airZone;
	static int[][] map;
	static boolean[][] visit;
	static boolean clear;
	static int H,W;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		H = sc.nextInt();
		W = sc.nextInt();
		map = new int[H][W];
		visit = new boolean[H][W];
		airZone = new LinkedList<int[]>();
		for(int row=0;row<H;row++)
			for(int col=0;col<W;col++) map[row][col] = sc.nextInt();
		
		//while(!clear){
			setAirzone();
			view();
		//}
		
	}
	private static void view() {
		for(int row=0;row<H;row++) {
			for(int col=0;col<W;col++) {
				System.out.print(visit[row][col]+" ");
			}
			System.out.println();
		}
		System.out.println();
	}
	private static void setAirzone() {
		for(int col=0;col<W;col++){
			for(int row=0;row<H;row++){
				if(map[row][col]==0 && visit[row][col]==false){
					airZone.add(new int[] {row,col});
					visit[row][col]= true;
				}
				else if(map[row][col]==1) break;
			}
		}
		
		for(int row=0;row<H;row++){
			for(int col=0;col<W;col++){
				if(map[row][col]==0 && visit[row][col]==false){
					airZone.add(new int[] {row,col});
					visit[row][col]= true;
				}
				else if(map[row][col]==1) break;
			}
		}
		
		for(int col=0;col<W;col++){
			for(int row=(H-1);row>=0;row--){
				if(map[row][col]==0 && visit[row][col]==false){
					airZone.add(new int[] {row,col});
					visit[row][col]= true;
				}
				else if(map[row][col]==1) break;
			}
		}
		
		for(int row=0;row<H;row++){
			for(int col=(W-1);col>=W;col--){
				if(map[row][col]==0 && visit[row][col]==false){
					airZone.add(new int[] {row,col});
					visit[row][col]= true;
				}
				else if(map[row][col]==1) break;
			}
		}
	}
}
