package CodingTest;

public class cleaning {
	
	static int[] dr = {0,1};
	static int[] dc = {1,0};//우 하
	static int[][] memo;
	static int R,C;
	public static void main(String[] args) {
		solution(3,2,new int[][] {{2,0,0},{2,0,0},{2,2,2}});
	}

	private static void solution(int N, int M, int[][] map) {
		memo = new int[N][M];
		R = N;
		C = M;
		memo[0][0]+=map[0][0];//초항
		dfs(0,0);
	}

	private static void dfs(int cr, int cc) {
		if(cr==(R-1) && cc==(C-1)) {
			return ; 
		}
		int nr = cr;
		int nc = cc;
		
		for(int dir=0;dir<2;dir++) {
			nr = cr+dr[dir];
			nc = cc+dc[dir];
		}
		
		
	}
}
