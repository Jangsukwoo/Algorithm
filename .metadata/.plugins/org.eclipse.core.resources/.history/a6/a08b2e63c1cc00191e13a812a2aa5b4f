	static char[] direction= {'U','R','D','L'};
	static boolean[][][] checkMap;
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		Map = new char[N][M];
		checkMap = new boolean[N][M][4];
		sc.nextLine();
		for(int i=0; i<N;i++) Map[i] = sc.nextLine().toCharArray();
		PR = sc.nextInt();
		PC = sc.nextInt();
		
	
	}
}
