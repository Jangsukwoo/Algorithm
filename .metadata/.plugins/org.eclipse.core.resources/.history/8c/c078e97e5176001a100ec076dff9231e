package 프로그래머스;

import java.util.LinkedList;
import java.util.Queue;

public class 블록이동하기 {
	static class Robot{
		int leftR,leftC,rightR,rightC;
		int position;//가로 : 1, 세로 : 2
		public Robot(int leftR, int leftC, int rightR, int rightC, int position) {
			this.leftR = leftR;
			this.leftC = leftC;
			this.rightR = rightR;
			this.rightC = rightC;
			this.position = position;
		}
	}
	public static void main(String[] args) {

	}
    public  static int solution(int[][] board) {
        int answer = 0;
        Queue<Robot> q = new LinkedList<Robot>();
        q.add(new Robot(0,0,0,1,1));
        return answer;
    }
}
