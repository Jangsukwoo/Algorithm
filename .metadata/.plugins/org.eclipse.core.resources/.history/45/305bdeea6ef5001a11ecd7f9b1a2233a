import java.util.Deque;
import java.util.LinkedList;

class 라2 {
	static Deque<Integer> dq = new LinkedList<Integer>();
	public int[] solution(int[] ball, int[] order) {
		int[] answer = new int[ball.length];

		for (int i = 0; i < ball.length; i++)  dq.add(ball[i]);//데크에 삽입 
		int time = 0;
		int k = 0;
		while (!dq.isEmpty()) {

			int target = order[time];
			
			if (dq.peekFirst() == target){//앞에 꺼내려는 볼 번호면 
				answer[k] = target;
				dq.pollFirst(); //꺼
				order[time] = 0;
				time = 0;
				k++;
			} else if (dq.peekLast() == target) {//뒤에 꺼내려는 볼 번호가 있으면
				order[time] = 0;
				answer[k] = chck;
				dq.pollLast();
				x = 0;
				k++;

			} else {
				time++;
			}

		}
		return answer;
	}
	public static void main(String[] args) {

	}
}