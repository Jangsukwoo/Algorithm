package SWExpertAcademy;

import java.util.Scanner;
/*
 * 두 손님의 식성에 최대한 비슷한 음식 만들기
 * 식재료 N개, N은 짝수
 * 식재료 N/2를 해서 두개의 요리를 만들려 함.
 * 음식은 A,B
 * 맛의 차이가 최소가 되도록 재료를 배분해야함
 * 식재료 i와 j에 대해서 시너지 Sij 발생.
 * Sij 테이블이 주어짐.
 * 두 음식간의 맛의 차이가 최소가 되는 경우를 찾고 최소값을 정답으로 출력
 */
public class 요리사 {
	static int[][] synergyTable;
	static int N;
	static int[] data;
	static int r;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int testcase=1;testcase<=T;testcase++){
			N = sc.nextInt();
			r = N/2;
			data = new int[r];
			synergyTable = new int[N][N];
			for(int i=0;i<N;i++)
				for(int j=0;j<N;j++) synergyTable[i][j] = sc.nextInt();
			nCr(0,0);
		}
	}
	private static void nCr(int idx,int cnt){
		if(cnt==r){
			return;
		}
		for(int i=idx;i<N;i++){
			data[cnt] = i+1;
			nCr(i+1,cnt+1);
		}
	}
}
