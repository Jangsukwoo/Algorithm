package CodingAcademy;

public class 변리사_3번 {
	public static void main(String[] args) {
		int[] data = new int[] {0,9,8,5,2,4,1,3};
		int size = data.length;
		AAASort(data,size);
	}

	private static void AAASort(int[] a, int size) {
		int n = size-1;
		int temp=0;
		for(int i = n/2; i>=2 ;i--) {
			BBB(a,i,n);
		}
		for(int i=n;i>=2;i--) {
			BBB(a,1,i-1);
			temp = a[1];
			/*
			 * To do
			 */
		}
	}

	private static void BBB(int[] a, int h, int m) {
		int imsi = a[h];
		
	}
}
