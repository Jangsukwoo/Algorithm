		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		dp = new int[N];
		cards = new int[N];
		Arrays.fill(dp,1);//일단 1로 채움
		for(int i=0;i<N;i++) cards[i] = sc.nextInt();
		for(int i=0;i<N;i++){
			
			for(int j=0;j<i;j++){
				
			}
		}
	}
}
