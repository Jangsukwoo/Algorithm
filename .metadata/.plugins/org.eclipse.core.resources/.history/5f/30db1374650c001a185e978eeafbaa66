package BAEKJOON;

import java.util.Scanner;

public class 수이어쓰기1 {
	static int N;
	static long size;
	static StringBuilder sb = new StringBuilder();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		for(int i=1;i<=N;i++){
			String data = Integer.toString(i);
			size+=data.length();
		}
		System.out.println(size);
	}
}
