package Samsung;

import java.util.Scanner;

class SharkInfo{
	int id;
	int row;
	int col;
	int speed;
	int dir;
	int size;
	public SharkInfo(int i, int r, int c, int s, int d, int z){
		id = i;
		row =r;
		col =c;
		speed = s;
		dir = d;
		size = z;
	}
}
public class 기출_낚시왕 {
	static int R,C,M;
	static int SharkSize;
	static int[][] sea;
	static SharkInfo[] sharks;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		R = sc.nextInt();
		C = sc.nextInt();
		M = sc.nextInt();
		sharks = new SharkInfo[M];
		for(int i=1;i<=M;i++){
			int r = sc.nextInt();
			int c = sc.nextInt();
			int s = sc.nextInt();
			int d = sc.nextInt();
			int z = sc.nextInt();
			sharks[i-1] = new SharkInfo(i,r, c, s, d, z);
			sea[r][c] = i;
		}
		for(int move=0;move<C;move++){//낚시꾼 이동
			for(int row=0;row<R;row++) {
				if(sea[row][move]!=0){
					SharkSize+=sharks[sea[row][move]-1].size;
				}
			}
		}
	}
}

