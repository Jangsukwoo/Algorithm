package Samsung;
import java.util.ArrayList;

/*
 * 시작 200
 * 
 */
import java.util.Scanner;
class Pinball{
	int row;
	int col;
	int dir;
	public Pinball(int r, int c, int d){
		row = r;
		col = c;
		dir =d;
	}
}
class Wormhole{
	int row;
	int col;
	public Wormhole(int r,int c) {
		row =r;
		col =c;
	}
}
public class 모의_핀볼게임 {
	static int[] dr = {-1,0,1,0};
	static int[] dc = {0,1,0,-1};//상우하좌
	static int N;
	static int[][] board;
	static int maxscore;
	static Wormhole[][] wormholes;
	static int[] wormholeCount;
	static int startR, startC;
	static ArrayList<Pinball> startList = new ArrayList<Pinball>();
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int testcase=1;testcase<=T;testcase++){
			N = sc.nextInt();
			board = new int[N][N];
			maxscore=0;
			wormholes = new Wormhole[5][2]; 
			wormholeCount = new int[5];
			for(int row=0;row<N;row++) {
				for(int col=0;col<N;col++) {
					board[row][col] = sc.nextInt();
					if(board[row][col]==0){
						startList.add(new Pinball(row,col,0));
					}
					else if(board[row][col]>=6 && board[row][col]<=10){
						if(wormholeCount[board[row][col]-6]==0){
							wormholes[board[row][col]-6][0] = new Wormhole(row,col);
						}else {							
							wormholes[board[row][col]-6][1] = new Wormhole(row,col);
						}
					}
				}
			}
			int size = startList.size();
			for(int i=0;i<size;i++){
				for(int dir=0;dir<4;dir++){
					Pinball curPinball = startList.get(i);
					startR = curPinball.row;
					startC = curPinball.col;
					int nr = curPinball.row+dr[dir];
					int nc = curPinball.col+dc[dir];
					dfs(nr,nc,0,dir);
				}
			}
			System.out.println("#"+testcase+" "+maxscore);
		}
	}
	private static void dfs(int nr, int nc, int score, int dir) {
		if(nr==-1){ //벽을 만나는 4가지의 경우
			nr= 0;
			dir = (dir+2)%4;//대칭
			dfs(nr,nc,score+1,dir);
		}else if(nr==N){
			nr= N-1;
			dir = (dir+2)%4;//대칭
			dfs(nr,nc,score+1,dir);	
		}
		if(nc==-1){
			nc= 0;
			dir = (dir+2)%4;//대칭
			dfs(nr,nc,score+1,dir);
		}else if(nc==N){
			nc= N-1;
			dir = (dir+2)%4;//대칭
			dfs(nr,nc,score+1,dir);	
		}else {
			if(board[nr][nc]== -1 || (nr==startR && nc==startC)){// 블랙홀이거나 시작점이라면
				maxscore = Math.max(maxscore,score);
				return ;
			}else {
				switch (board[nr][nc]) {
				case 0:					
					dfs(nr+dr[dir],nc+dc[dir],score,dir);
					break;
				case 1:		
					switch (dir) {
					case 0: case 1:
						dir = (dir+2)%4;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 2:
						dir = 1;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 3:
						dir = 0;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					}
					
					break;
				case 2:	
					switch (dir) {
					case 1: case 2:
						dir = (dir+2)%4;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 0:
						dir = 1;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 3:
						dir = 2;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					}
					break;
				case 3:		
					switch (dir) {
					case 2: case 3:
						dir = (dir+2)%4;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 0:
						dir = 3;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 1:
						dir = 2;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					}
					break;
				case 4:		
					switch (dir) {
					case 0: case 3:
						dir = (dir+2)%4;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 1:
						dir = 0;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					case 2:
						dir = 3;
						dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
						break;
					}
					break;
				case 5:		
					dir = (dir+2)%4;
					dfs(nr+dr[dir],nc+dc[dir],score+1,dir);
					break;
				case 6:case 7:case 8:case 9:case 10:	
					if(wormholes[board[nr][nc]-6][0].row==nr && wormholes[board[nr][nc]-6][0].col==nc){
						nr = wormholes[board[nr][nc]-6][1].row;
						nc =  wormholes[board[nr][nc]-6][1].col;
						dfs(nr,nc,score,dir);
					}else {
						nr = wormholes[board[nr][nc]-6][0].row;
						nc = wormholes[board[nr][nc]-6][0].col;
						dfs(nr,nc,score,dir);
					}
					break;
				}
			}
		}
	}
}
